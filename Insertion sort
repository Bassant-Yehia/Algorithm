#include <iostream>

using namespace std;

void Insertion()

{
int n,temp;
      cin>>n;
      int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }

 for(int i=1;i<n;i++)
 {
    int sorted_turn=arr[i];
    int j;
    for(j=i-1;j>=0&&arr[j]>sorted_turn;j--)
    {

        int temp =arr[j];
         arr[j]=arr[j+1];
         arr[j+1]=temp;
}

     arr[j+1]=sorted_turn;
 }

 for(int k=0;k<n;k++)
 {
     cout<<k+1<<") "<<arr[k]<<endl;
 }

}
int main()
{

     Insertion();

    return 0;
}
